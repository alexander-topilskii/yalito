{
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
       {
        "source": "/test",
        "destination": "/test.html"
      },
       {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
